[[include :topia:pride-animated-source]]
[[module css]]
@import url("http://scp-jp.wdfiles.com/local--code/fragment%3Ablack-highlighter-theme-source/1");
@import url("http://scp-jp.wdfiles.com/local--code/fragment%3Ablack-highlighter-theme-source/2");
@import url("http://scp-jp.wdfiles.com/local--code/fragment%3Ablack-highlighter-theme-source/3");

:root {
    --logo-image: url("https://scp-wiki.wdfiles.com/local--files/component:pride-highlighter/lgbtqp_logo.svg") !important;
}

#page-title {
    text-align: center;
}

h1 {
    display: flex;
    justify-content: center;
}

div.pridebox,
div.includebox {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
}

div.pridebox div.insidebox {
    flex-basis: calc((100% / 2) - 0.125rem);
    border: 0.0625rem solid rgb(var(--bright-accent));
    flex-grow: 2;
    min-width: 18.750em;
}

div.insidebox div.imagebox {
    display: flex;
    justify-content: center;
}

div.insidebox div.imagebox > div {
    width: 50%;
    min-width: inherit;
}

div.includebox {
    text-align: center;
}

div.includebox > blockquote {
    flex-basis: calc(50% - 1em);
    flex-grow: 2;
    box-sizing: border-box;
    margin: 0.25em;
    width: calc(50% - 1em);
}

div.includebox tt {
    font-size: 80%;
}

div.pridebox div.insidebox h1, div.pridebox div.insidebox h2 {
    --wght: 900;
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: 900;
    padding: 0 1em;
}

#page-content > div.pridebox div.scp-image-block {
    box-shadow: inherit;
    background-color: rgb(var(--very-light-gray-monochrome));
    border: initial;
}

#page-content > div.pridebox div.scp-image-block > a {
    width: 90%;
    padding: 1rem 0;
}

#page-content > div.pridebox div.scp-image-block > div.scp-image-caption {
    border-top: inherit;
}

.flipped {
    display: inline-block;
    transform: scaleX(-1);
}

[[/module]]

+ [[span class="translation-source"]]🌈 BHL + Sigma 9 Pride Logos! [[span class="flipped"]]🌈[[/span]][[/span]]

[[div clas="blockquote"]]
[[span class="translation-source"]]//Disclaimer: This is 100% not endorsed by SCP Staff in any way. I am an SCP Author who has decided to create these for other SCP Authors to use in celebration of Pride month (and beyond if they so choose.)// [[/span]]
[[/div]]

