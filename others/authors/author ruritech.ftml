[[module CSS]]
.works-container .work {
    display: grid;
    margin: 10px 5px;
    border: 2px solid #444444;
    box-shadow: #000c 0px 2px 4px;
    grid-template-rows: min-content min-content min-content 1fr;
    grid-template-columns: 1fr 143px;
    transition: box-shadow 0.75s;
    position: relative;
    box-sizing: border-box;
}
.works-container .work:hover {
    box-shadow: #000c 0px 2px 4px 1px;
}
.work .title {
    font-weight: bold;
    font-size: 0.75em;
    grid-area: title;
    padding: 0.25em 0.75em;
    background: #fff;
    font-weight: bold;
}
.work .metatitle {
    grid-area: metatitle;
    background: #B81818;
    font-weight: bold;
    color: #fff;
}
.work .photo {
    grid-area: photo;
}
.work .photo div.image {
    background-position: center;
    background-size: cover;
    background-color: #FFF;
}
.work .text {
    font-size: 0.75em;
    grid-area: text;
    padding: 0.5em 0.5em;
    background: #000;
    color: #fff;
}
.work .link a {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    font-size: 0;
}
@media (max-width: 479px) {
    .works-container .work {
        grid-template-areas:
            "title photo"
            "metatitle photo"
            "text photo"
            "text photo";
    }
    .work .photo {
        overflow: hidden;
    }
    .work .photo div.image {
        height: 100%;
    }}
@media (min-width: 480px) {
    .works-container {
        display: flex;
        flex-wrap: wrap;
    }
    .works-container .work {
        width: calc(100% / 2 - 10px);
        grid-template-areas:
            "title title"
            "photo photo"
