/* ここからターゲットによる表示変換 */
#u-none:target ~ .before, #u-none ~ .after {display: none;}
#u-none ~ .before, #u-none:target ~ .after {display: block;}
/* ここまで */

/* ここからチャレンジの疑似囲い */
.blockquoto {
