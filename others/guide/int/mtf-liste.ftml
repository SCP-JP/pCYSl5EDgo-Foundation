[[module css]]
@import url('http://scp-wiki-de.wdfiles.com/local--code/component:image-pop-up/2');

#main-content {
    padding-top: 0;
    top: 0.5em;
}
#page-title {
    display: none;
}
#u-buttonbox {
    background: transparent;
    border: 0;
    box-shadow: none;
    float: right;
    margin: 0;
    padding: 0;
    position: absolute;
    top: .5em;
    right: 1.5em;
}

.wp_sheet.c_list-box {
    margin-bottom: 3em;
    background-image: linear-gradient(to bottom, #eee 4em, #fff 4em);
}
.c_intro-toc {
    margin-top: 2em;
    text-align: center;
    background-image: none;
}
.long-toc {
    float: none;
    margin: 20px 0;
}
.long-toc .toc-list {
    columns: 2;
}
.wp_sheet .image-container {
    padding-top: 0;
}
.wp_sheet img {
    height: 240px;
    object-fit: contain;
}
.c_list-box p {
    margin-bottom: .2em;
    margin-top: .5em;
}
.c_list-box ul {
    margin-top: 0;
}
.c_list-box h1 {
    display: block;
    text-align: center;
    margin-bottom: 1.4em;
    color: #333;
    font-family: "Trebuchet MS", Trebuchet, Verdana, Arial, Helvetica;
    font-size: 160%
}

.justify li {
    text-align: initial;
}
.justify td {
    text-align: justify;
}

.c_list-box .pop-button .image-container {
    padding: 0 0 0 1em;
}
.c_list-box .pop-button img {
    height: 135px;
    -o-object-fit: contain;
    object-fit: contain;
}
.c_beschreibung p:first-child {
    margin-top: 0;
}
.c_overview p:first-child {
    text-align: center;
    font-size: 110%;
}
.c_overview ul {
    margin-bottom: 1em;
}
.white-paper .col-title .colmod-content .foldable-list-container {
    font-size: initial;
}
.wp_sheet #toc, .wp_sheet .toc {
    margin-bottom: 1em;
}
.wp_sheet .long-toc {
    margin: 1em 0 20px;
    padding: .5em 1.2em;
}
.toc-list p:not(:first-child) {
    margin-top: .5em;
}
#u-RTFs {
    background-image: none;
}
#u-RTFs.c_list-box p {
    margin: 1em 0;
}
#u-RTFs .justify.hyphens {
    margin-bottom: 1em;
}

/* Site box footer */
.wp_sheet .panel-footer {
    clear: both;
    padding: 1px;
    color: #fffff0;
    font-size: 80%;
    font-weight: bold;
    padding: 1px 0px 4px;
    text-align: right;
    text-shadow: 1px 1px 2px rgba(0,0,0,.5);
}
.wp_sheet .panel-footer > p {
    margin: 0;
}
.wp_sheet .panel-footer a,
.wp_sheet .panel-footer a:visited {
    color:#888880;
    text-shadow: 0px 0px 1px rgba(0,0,0,.1);
}

@media (max-width: 880px) {
    .c_list-box td {
     display: block;
    }
}
@media (min-width: 880px) {
    .c_list-box .image-container {
     padding: 0;
    }
    .c_list-box td:first-of-type {
     width: 200px;
     padding-right: 1em;
    }
    .c_list-box img {
     height: auto;
    }
}
@media (min-width: 768px) and (max-width: 979px) {
    .long-toc .toc-list {
     columns: 1;
    }
}
@media (max-width: 710px) {
    .long-toc .toc-list {
     columns: 1;
    }
}
@media (min-width: 768px) and (max-width: 880px) {
    .wp_title {
        font-size: 285%;
    }
}
@media (max-width: 595px) {
    .wp_title {
        font-size: 285%;
    }
    .c_list-box h1 {
     font-size: 170%;
     color: #901;
     color: var(--ct_col_h1-txt);
    }
    .c_list-box {
     background-image: none;
    }
}
@media (max-width: 767px) {
    #u-buttonbox {
        right: .5em;
    }
}

/* Popup */
.c_map .pop-wrap .pop-popup {
    animation: fadein 0.35s ease-in forwards;
    background: #888880;
    border: 1px solid #888880;
    border-radius: 10px;
    height: auto;
    left: 0;
    margin: 2em auto;
    position: fixed;
    right: 0;
    top: 2em;
    width: 1024px;
    z-index: 10;
}

.c_list-box .pop-wrap .pop-popup {
    animation: fadein 0.35s ease-in forwards;
    left: 0;
    margin: 0 auto;
    position: absolute;
    right: 0;
    width: 353px;
    z-index: 10;
}

#page-content .c_map .pop-wrap div.pop-popup {
    max-width: 95%;
}

.c_map .pop-wrap .pop-button div.foldable-list-container {
    display: block;
}
.c_list-box .pop-wrap {
    float: right;
}
.pop-wrap br {
    line-height: 0;
}
.c_map .pop-wrap .pop-popup img {
    border-radius: 10px;
}
.c_list-box .pop-wrap .pop-popup .image-container {
    padding: 0;
}

.c_list-box table.wiki-content-table {
    margin: 0.5em 0;
}
.c_list-box table.wiki-content-table td {
    border: 0;
    padding-left: 0;
    padding-right: 2em;
    width: auto;
}
[[/module]]

