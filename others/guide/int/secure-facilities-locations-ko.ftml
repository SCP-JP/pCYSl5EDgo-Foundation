[[module CSS]]
.siteIcon {
  display: inline-block;
  position: relative;
  top: 1px;
  width: 30px;
  height: 17px;
  vertical-align: bottom;
  background-color: #999;
  border: solid 2px #000;
  border-radius: 0 6px 0 6px;
  color: #fff;
}
.siteIcon img {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border: 0;
}
.siteIcon .number {
  position: absolute;
  bottom: 2px;
  right: 2px;
  line-height: 100%;
  font-size: 60%;
  font-weight: bold;
  text-shadow: #333 1px 1px;
}

.areaIcon {
  display: inline-block;
  position: relative;
  top: 1px;
  width: 30px;
  height: 17px;
  vertical-align: bottom;
  background-color: #333;
  border: solid 2px #000;
  border-radius: 0 6px 0 6px;
  color: #fe3;
}
.areaIcon img {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border: 0;
}
.areaIcon .number {
  position: absolute;
  bottom: 2px;
  right: 2px;
  line-height: 100%;
  font-size: 60%;
  font-weight: bold;
  text-shadow: #333 1px 1px;
}
