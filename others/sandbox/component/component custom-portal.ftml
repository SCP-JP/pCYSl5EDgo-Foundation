[[module css]]
.system-draft-list-show-area{
  display: none;
}

#page-title, #page-content {
  font-weight: 400;
}

.portal-content-panel {
    background-color: white;
    border-radius: 0px;
    margin: 0 1em;
    padding: 0.1em;
    font-weight: 400;
    border: solid 1px gray;
    box-shadow: none;
}

.custom-sb3-portal {
    margin: 1.5em 0 1.5em 0;
}

.custom-sb3-portal ol li {
    margin: 0 0 0.1em 0;
}

.ab,
.ba {
    display: none;
}

.custom-undefined-draftList,
.custom-writting-draftList,
.custom-wreforming-draftList,
.custom-wcriticismin-draftList,
.custom-wcriticismend-draftList,
.custom-translating-draftList,
.custom-treforming-draftList,
.custom-tcriticismin-draftList,
.custom-tcriticismend-draftList,
.custom-others-draftList,
.custom-oreforming-draftList,
.custom-ocriticismin-draftList,
.custom-ocriticismend-draftList,
.custom-sharing-draftList {
    display:none !important;
    border-bottom: solid 1px gray;
    padding-bottom: 5px;
}

.custom-scp-draftList,
.custom-tale-draftList,
.custom-goif-draftList {
    display:none !important;
}

.custom-undefined-draftList p,
.custom-writting-draftList p,
.custom-wreforming-draftList p,
.custom-wcriticismin-draftList p,
.custom-wcriticismend-draftList p,
.custom-translating-draftList p,
.custom-treforming-draftList p,
.custom-tcriticismin-draftList p,
.custom-tcriticismend-draftList p,
.custom-others-draftList p,
.custom-oreforming-draftList p,
.custom-ocriticismin-draftList p,
.custom-ocriticismend-draftList p,
.custom-sharing-draftList p {
    padding:0;
    margin:0;
}

.undefined-title,
.writting-title,
.wreforming-title,
.wcriticismin-title,
.wcriticismend-title,
.translating-title,
.treforming-title,
.tcriticismin-title,
.tcriticismend-title,
.others-title,
.oreforming-title,
.ocriticismin-title,
.ocriticismend-title,
.sharing-title {
    font-weight:bold;
    font-size: 150%;
    margin: 0.5em 0 0.4em;
    padding: 0;
    letter-spacing: 1px;
}

.undefined-title:before {
    content: "\f0f6";
    font-family: FontAwesome;
    margin-right: 3px;
    color: black;
}

.writting-title:before {
    content: "\f040";
    font-family: FontAwesome;
    margin-right: 3px;
    color: black;
}

.wreforming-title:before {
