    rgba(var(--background-gradient-color), 0.825) 11%,
    rgba(var(--background-gradient-color), 0.741) 16.6%,
    rgba(var(--background-gradient-color), 0.648) 23.1%,
    rgba(var(--background-gradient-color), 0.550) 30.4%,
