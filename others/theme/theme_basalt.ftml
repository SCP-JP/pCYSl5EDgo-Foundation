    }
    .document,
    .darkdocument {
        width: 95%;
    }
}
 
@media (max-width: 767px) {
    .mobile-top-bar {
        display: flex;
        justify-content: flex-start;
        max-width: 100%;
        width: 100%;
        flex-direction: row;
    }
}
 
@media (max-width: 500px) {
    .logo {
        display: none;
    }
    #top-bar {
        left: 3.6rem;
    }
    #extra-div-1::before,
    #extra-div-2::before {
        left: 4.2rem;
    }
    #u-header-link {
        left: 4rem;
    }
    .document, .darkdocument {
        width: auto;
    }
