[[module css]]
.icons {
    display: grid;
    grid-template-columns: repeat(5, calc((100% / 5) - 0.25rem));
    grid-gap: 0.25rem;
}

@media (max-width: 500px ) {
    .icons {
     grid-template-columns: repeat(3, calc((100% / 3) - 0.25rem));
    }
}

.icon_image {
    border-radius: 50%;
    border: 0.25rem solid  rgb(0,0,0);
    overflow: hidden;
}

.icon_name,
.icon_link {
    display: flex;
    align-items: center;
    justify-content: center;
    text-transform: uppercase;
}

.footnotes-footer {
display:none;
}
[[/module]]