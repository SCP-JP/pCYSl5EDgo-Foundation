[[module css]]
.noteabstruct {
width:100%;
display:block;
margin:auto;
}

.noteabstruct tr td{
 width:calc(50% - 2px);
vertical-align:top;
}

.noteabstruct tr td table{
display:block;
 height:100%;
width:100%;
}

@media (max-width: 479px) {
.blacnk-Record{
min-height:1em !important;
}
.noteabstruct table td{
display:block;
min-height:2em;
width:calc(100% - 3px);
}
}
[[/module]]