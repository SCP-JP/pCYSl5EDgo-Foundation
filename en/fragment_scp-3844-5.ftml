<script type="text/javascript">
function addEvent(element, eventName, callback) {
    if (element.addEventListener) {
        element.addEventListener(eventName, callback, false);
    } else if (element.attachEvent) {
        element.attachEvent("on" + eventName, callback);
    } else {
        element["on" + eventName] = callback;
    }
}

window.toggle = function(cls) {
  var collapsed = document.querySelectorAll("." + cls + ".collapsed");
  var expanded = document.querySelectorAll("." + cls + ":not(.collapsed)");
  var i;
  for(i = 0; i < collapsed.length; i++) {
    collapsed[i].classList.remove("collapsed");
  }
  for(i = 0; i < expanded.length; i++) {
    expanded[i].classList.add("collapsed");
  }
};


window.onload = function() { 
  var addADot = function(){$('#loadingpage').text($('#loadingpage').text() + '.');};
   n =  new Date();
  y = n.getFullYear();
  m = n.getMonth() + 1;
  d = n.getDate();
  var addDate = function(){$('#fillin').text($('#fillin').text() + "ページ リビジョン 5/5 from "+ y + "/" + m + "/" + d);};
  setTimeout(addADot,750);
  setTimeout(addADot,750*2);
  setTimeout(addADot,750*3);
  setTimeout(function(){addDate(); document.getElementById('doc').classList.remove('vanished');},750*4);


  window.switchIt = function() {
    document.getElementById('doc').classList.add('vanished');
    $('#loadingpage').text('Uploading new iteration');
      setTimeout(addADot,750);
  setTimeout(addADot,750*2);
  setTimeout(addADot,750*3);
    setTimeout(function(){document.getElementById('rev').classList.remove('vanished');},750*4);
 
  }
};



</script>

