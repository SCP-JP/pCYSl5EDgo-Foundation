}
 .intscpfn-1{
     margin: 0 auto;

     color: hsl(0, 100%, 100%);
     background-color: hsla(0, 100%, 20%);
     border: solid 1px hsla(0, 100%, 20%);
     box-shadow:
       inset 0 1px hsla(0, 100%, 100%, .5),
       inset 0 10px hsla(0, 100%, 90%, .5),
       inset 0 10px 20px hsla(0, 100%, 90%, .3),
       inset 0 -15px 30px hsla(0, 100%, 9%, .5);

     font-size: 150%;
}
 .intscpfn-2{
     margin: 1.563em auto 0;

     background-color: hsl(0, 100%, 97%);
     border: 1px solid hsla(0, 100%, 20%);
}
 .intscpfn-3{
     margin: 1.563em auto 0;

     background-color: hsl(60, 100%, 97%);
     border: 1px solid hsl(60, 100%, 20%);
}
 @media (max-width: 479px){
     .intscpfn-1, .intscpfn-2, .intscpfn-3{
         width: unset;
    }
