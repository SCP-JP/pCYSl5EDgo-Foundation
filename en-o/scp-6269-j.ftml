    --four-color: 194, 85, 0 !important;
}

.list-pages-item > hr,
#page-content > hr {
background: transparent;
border-top: double 3px var(--accentColor);
filter: brightness(170%);
}

.custom2 .wiki-content-table {
    width: 100%;
}

#page-content tr th {
    text-align: center;
}

.custom2 tr td {
    padding: 10px !important;
}

.fnnum::after {
    color: #699bc4 !important;
}
 
.fnnum:hover::after {
    color: white !important;
}
 
.fncon {
    background: rgb(25, 25, 25) !important;
    color: #EDEDED;
    border: 0.15rem solid rgb(20, 20, 20);
    border-radius: 2px;
    font-size: 90%;
}

::selection {
    background-color: rgba(227, 106, 0, 0.7);
