@media only screen and (max-width: 1300px) {
    .top-image {
    	margin-top: 0;
    	display: block;
    	margin-top: 4rem !important;
    }

    #page-content>div.rating-container {
    	position: relative;
    	transform: translateX(0);
    	margin-top: -2rem;
