[[module css]]
:root {
    --bal-right-color: #6DF560;
    --bal-left-color: #6DF560;
    --bal-inm-color: #0080FF;
    --bal-kin-color: #A4A4A4;
    --bal-com-color: #FFFF00;
    --bal-art-color: ;
    --bal-niku-color: #FAAC58;
    --bal-comn-color: #FFFF00;
}

.right {
    text-align: right;
}

.inm,.kin,.comn {
    text-align: right;
}



.com,.art,.niku {
    text-align: left;
}




.left {
    text-align: left;
}

.balloon,
.arrow {
    display: block;
    position: relative;
    width: 240px;
    color: #000000;
    font-family: Verdana, Arial, sans-serif;
    border-radius: 30px /70px;
    text-align: center;
    padding: 2px 20px;
}

.right > .balloon,
.right > .arrow {
    background-color: var(--bal-right-color);
    margin: 0 50px 0 auto;
}

.inm > .balloon,
.inm > .arrow {
    background-color: var(--bal-inm-color);
    margin: 0 50px 0 auto;
}

.kin > .balloon,
.kin > .arrow {
    background-color: var(--bal-kin-color);
    margin: 0 50px 0 auto;
}

.com > .balloon,
.com > .arrow {
    background-color: var(--bal-com-color);
    margin: 0 auto 0 50px;
}
.comn > .balloon,
.comn > .arrow {
    background-color: var(--bal-com-color);
    margin: 0 50px 0 auto;
}

.art > .balloon,
.art > .arrow {
    background:linear-gradient(to right,red,orange,yellow,green,aqua,blue,purple);
    margin: 0 auto 0 50px;
}

.niku > .balloon,
.niku > .arrow {
    background-color: var(--bal-niku-color);
    margin: 0 auto 0 50px;
}


.left > .balloon,
.left > .arrow {
    background-color: var(--bal-left-color);
    margin: 0 auto 0 50px;
}

.balloon:before {
    content: "";
    position: absolute;
    width: 13px;
    height: 12px;
    top: 0;
    border-radius: 50%;
}

.balloon:after {
    content: "";
    position: absolute;
    width: 20px;
    height: 18px;
    top: 3px;
    border-radius: 50%;
}

.right > .balloon:before,
.right > .balloon:after {
    background: var(--bal-right-color);
}

.right > .balloon:before {
    right: -38px;
}

.right > .balloon:after {
    right: -24px;
}

.inm > .balloon:before,
.inm > .balloon:after {
    background: var(--bal-inm-color);
}

.inm > .balloon:before {
    right: -38px;
}

.inm > .balloon:after {
    right: -24px;
}

.kin > .balloon:before,
.kin > .balloon:after {
    background: var(--bal-kin-color);
}

.kin > .balloon:before {
    right: -38px;
}

.kin > .balloon:after {
    right: -24px;
}


.comn > .balloon:before,
.comn > .balloon:after {
    background: var(--bal-comn-color);
}

.comn > .balloon:before {
    right: -38px;
}

.comn > .balloon:after {
    right: -24px;
}

.com > .balloon:before,
.com > .balloon:after {
    background: var(--bal-com-color);
}

.com > .balloon:before {
    left: -38px;
}

.com > .balloon:after {
    left: -24px;
}

.art > .balloon:before,
.art > .balloon:after {
    background: var(--bal-art-color);
}

.art > .balloon:before {
    left: -38px;
}

.art > .balloon:after {
    left: -24px;
}


.art > .balloon:before,
.art > .balloon:after {
    background: var(--bal-art-color);
}

.art > .balloon:before {
    left: -38px;
}

.art > .balloon:after {
    left: -24px;
}

.niku > .balloon:before,
.niku > .balloon:after {
    background: var(--bal-niku-color);
}

.niku > .balloon:before {
    left: -38px;
}

.niku > .balloon:after {
    left: -24px;
}


.arrow:before {
    content: "";
    position: absolute;
    top: -8px;
    margin-left: -15px;
    border: 10px solid transparent;
    z-index: 0;
    transform: scale(2);
}

.right > .arrow:before {
    border-left: 15px solid var(--bal-right-color);
    right: -18px;
    transform: scale(2) rotate(320deg);
}


.inm > .arrow:before {
    border-left: 15px solid var(--bal-inm-color);
    right: -18px;
    transform: scale(2) rotate(320deg);
}

.kin > .arrow:before {
    border-left: 15px solid var(--bal-kin-color);
    right: -18px;
    transform: scale(2) rotate(320deg);
}

.comn > .arrow:before {
    border-left: 15px solid var(--bal-comn-color);
    right: -18px;
    transform: scale(2) rotate(320deg);
}

.com > .arrow:before {
    border-right: 15px solid var(--bal-com-color);
    left: -3px;
    transform: scale(2) rotate(40deg);
}

.art > .arrow:before {
    border-right: 15px solid background:linear-gradient(to right,red,orange);
    left: -3px;
    transform: scale(2) rotate(40deg);
}

.niku > .arrow:before {
    border-right: 15px solid var(--bal-niku-color);
    left: -3px;
    transform: scale(2) rotate(40deg);
}


.left > .arrow:before {
    border-right: 15px solid var(--bal-left-color);
    left: -3px;
    transform: scale(2) rotate(40deg);
}

.kincom > .balloon,
.kincom > .arrow {
   background:linear-gradient(to right,#FFFF00,#A4A4A4);
    margin: 0 50px 0 auto;
}

.kincom > .arrow:before {
    border-left: 15px solid var(--bal-kin-color);
    right: -18px;
    transform: scale(2) rotate(320deg);
}

[[/module]]


