/* HEADER */
#header {
    height: 140px;
    position: relative;
    z-index: 10;
    padding-bottom: 30px; /* FOR MENU */
    background: url(http://scp-wiki.wdfiles.com/local--files/spc-001/SPCLogo50.png) 10px 35px no-repeat;
}

#header h1 a {
    content: "SPC サメ殴りセンター";
}

#header h1 a::before {
    content: "SPC サメ殴りセンター";
}

#header h1 a span{
    display: block;
    content: "";
    visibility: hidden;
}

#header h2 {
    margin-left: 120px;
    padding: 0;
    clear: left;
    float: left;
    font-size: 105%;
    max-height: 38px;
}
#header h2 span {
    display: block;
    margin: 0;
    padding: 19px 0;
    line-height: 0px;
    max-height: 0px;
    font-weight: bold;
    color: transparent;
    text-shadow: none;
}
#header h2 span::before {
    content: "索鮫 殴打 粉砕";
    display: block;
    margin: 0;
    padding: 0px 0;
    line-height: 0px;
    max-height: 0px;
    font-weight: bold;
    color: #f0f0c0;
    text-shadow: 1px 1px 1px #000;
    text-shadow: 1px 1px 1px rgba(0,0,0,.8);
}

div#container-wrap {
    background: url(http://scp-wiki.wdfiles.com/local--files/theme%3Aspc/SPCWrapper.jpg) top left repeat-x;
}
/* Source: https://www.flickr.com/photos/telemax/2818520042 */

