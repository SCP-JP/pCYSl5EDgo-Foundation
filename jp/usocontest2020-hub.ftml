div#header h1 {
  margin-left: unset;
  padding: unset;
  float: unset;
  max-height: unset;
}

div#header h1 a {
  position: relative;
  width: 300px;
  margin: 0 auto;
}

div#main-content {
  position: static;
}

#u-fake-header-title {
  font-family: 'Noto Serif JP', serif;
  font-weight: 900;
  display: block;
  position: absolute;
  top: -65px;
  left: 0;
  width: 100%;
  text-align: center;
  font-size: 17px;
  z-index: 9;
}

/* Collapsible Search Area */
#search-top-box {
  width: unset;
}

#search-top-box-form::after {
  position: absolute;
  z-index: 2;
  content: "\f002";
  font-family: "FontAwesome";
  top: 5px;
  left: 7.5px;
  font-size: 12px;
  color: white;
  pointer-events: none;
}

#search-top-box-input {
  position: relative;
  z-index: 1;
  width: 19px;
  height: 19px;
  border-radius: 35%;
  transition: .5s ease-out, z-index .1s step-start;
  background: rgba(0, 0, 0, 1);
}

#search-top-box-input:hover {
  background: rgba(34, 34, 34, 1)
}

#search-top-box input.empty {
  color: transparent;
}

#search-top-box-input:focus {
  z-index: 3;
  width: 100px;
  height: 19px;
  border-radius: unset;
  background: rgba(34, 34, 34, 1);
}

#search-top-box-form .btn {
  width: 1px;
  opacity: 0;
  transition: .5s ease-out;
}

@media (max-width: 479px) {
  #search-top-box {
    top: 79px;
    right: 9px;
  }
}

/* Background */
body {
  background: rgba(37, 37, 37, 1);
}

/* Colors */
body {
  color: white;
}

a {
  color: var(--c-accent);
  text-shadow: 2px 1.5px 2px black;
}

a.newpage {
  color: maroon;
}

#page-title, h1 {
  color: var(--c-accent);
  text-shadow: var(--c-accent-shadow);
  font-family: 'Noto Serif JP', serif;
}

blockquote, .blockquote {
  background: rgba(255, 255, 255, 0.05);
  border: solid 0.2px rgba(255, 255, 255, 0.1)
}

/*Tabs*/
#page-content .yui-nav {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  border: unset;
  border-bottom: solid 2px rgba(255, 255, 255, 0.15);
}

#page-content .yui-nav li {
  flex: 1 1;
  display: inline-block;
  margin: 0;
  padding: 0;
  max-width: 100%;
}

#page-content .yui-nav li a {
  width: 100%;
  white-space: nowrap;
  text-align: center;
}

#page-content .yui-nav a {
  background: rgba(255, 255, 255, 0.05);
  color: white;
}

#page-content .yui-nav a:hover,
#page-content .yui-nav .selected a,
#page-content .yui-nav .selected a:focus,
#page-content .yui-nav .selected a:hover {
  background: rgba(255, 255, 255, 0.1);
  color: white;
}

#page-content .yui-nav li * {
  border: unset;
}

#page-content .yui-nav li a em {
  padding 0;
  top: unset;
  padding: 0.4em 1em;
}

#page-content .yui-content {
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-top-color: rgba(255, 255, 255, 0.3);
  background: rgba(255, 255, 255, 0.05);
}

#html-body .hovertip {
  background: rgba(37, 37, 37, 1);
  border: solid 0.5px rgba(255, 255, 255, .5);
  color: white;
}

#html-body .hovertip .content .footnote {
  color: black;
}

/* Side-Bar */
#side-bar,
#top-bar,
#u-open-side-link,
#u-close-side-link {
  display: none;
}

#u-open-side-link a {
  display: inline-block;
  position: fixed;
  bottom: 1em;
  left: 1em;
  width: 30px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  font-size: 20px;
  color: rgba(255, 255, 255, 0.75);
  border-radius: 5px;
  border: solid 1px rgba(255, 255, 255, 0.25);
  text-decoration: none;
  background: rgba(30, 30, 30, 1);
}

#u-open-side-link a:hover {
  background: rgba(50, 50, 50, 1);
}

#u-side-nav {
  position: absolute;
  top: 1.5em;
  left: 1em;
  float: left;
  clear: left;
  margin: 0;
  padding: 0;
  font-family: 'Noto Serif JP', serif;
  font-weight: 400;
}

#u-side-nav hr {
  margin: 0.25em 2em;
  background-color: rgba(255, 255, 255, 0.5);
}

#u-side-nav p {
  margin: 0;
}

#u-side-nav a,
#u-side-nav a.newpage {
  color: var(--c-accent);
}

#u-side-nav .side-heading {
  width: 14em;
  padding: 0.25em 0 0.25em 0.25em;
  margin-bottom: 0.25em;
  border-bottom: solid 1px rgba(255, 255, 255, 0.5);
  font-size: 120%;
  font-weight: 700;
  color: var(--c-accent);
  text-shadow: var(--c-accent-shadow);
}

#u-side-nav .side-content a {
  display: block;
  box-sizing: border-box;
  line-height: 2.5;
  padding-left: 1em;
  margin-left: .5em;
  text-decoration: none !important;
  transition: background 0.15s;
  color: white;
}

#u-side-nav .side-content a:hover {
  background: rgba(255, 255, 255, 0.1);
}

#u-side-nav .side-content .collapsible-block-unfolded-link a {
  line-height: 2;
  padding-left: 0;
  margin-left: .5em;
  padding-left: .5em;
  color: white;
  border-bottom: solid 1px rgba(255, 255, 255, 0.5);
  text-decoration: none !important;
  background: none !important;
}

#u-side-nav .side-content br {
  display: none;
}

#u-side-nav .collapsible-block-content {
  margin-left: .5em;
}

#u-side-nav .scps .collapsible-block-content p {
  display: flex;
  justify-content: space-between;
  align-items: stretch;
}

#u-side-nav .scps .collapsible-block-content a {
  display: inline-block;
  text-align: center;
  flex: 1 1 40%;
  padding-left: unset;
  margin-left: unset;
}

@media(max-width:767px) {
  #u-open-side-link {
    display: block;
  }

  #u-close-side-link a {
    display: block;
    font-size: 0px;
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.25);
    z-index: 998;
  }

  #u-side-nav {
    position: fixed;
    top: 0;
    left: 0;
    height: 100vh;
    padding: 1em;
    background: rgba(50, 50, 50, 1);
    transform: translateX(-300px);
    transition: transform 0.5s ease-out;
    z-index: 999;
    overflow-y: scroll;
  }

  #u-side-nav:target {
    transform: translateX(0px);
  }

  #u-side-nav:target~#u-close-side-link {
    display: block;
  }
}

#odialog-container {
  color: black;
}

/*決選投票ボックス*/
.finalvoting {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: stretch;
  align-content: stretch;
}

.finalvoting_article {
  box-sizing: border-box;
  min-width: 250px;
  max-width: 341px!important;
  flex: 1 1 40%;
  border: dashed .25px darkgray;
  margin: .5em;
  padding: 1em;
}


