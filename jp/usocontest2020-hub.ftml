[[module css]]
@import url(http://scp-jp.wdfiles.com/local--code_/usocontest2020-hub/1);

.result-wrap {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: stretch;
  align-content: stretch;
  white-space: nowrap;
}

.result-wrap .result.first {
  display: flex;
  justify-content: center;
  flex: 1 1 100%;
  border: solid .1px dimgray;
  margin: .25em;
  box-sizing: border-box;
  height: 100px;
}

.result-wrap .result.first .goldpanel {
  flex: 0 0 100px;
  line-height: 100px;
  text-align: center;
  font-size: 50px;
  font-family: "FontAwesome";
  color: white;
  background: linear-gradient(150deg, #BF953F, #f5ec94, #B38728, #f5ec94);
  text-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}

.result-wrap .result.first .title {
  flex: 1 1 70%;
  display: flex;
}

.result-wrap .result.first .title .awardtitle {
  flex: 1 1 40%;
  line-height: 100px;
  font-size: 150%;
  text-align: center;
}

.result-wrap .result.first .title a {
  display: block;
  flex: 1 1 60%;
  line-height: 100px;
  font-size: 150%;
  text-align: center;
}

.result-wrap .result.first .articledetail {
  flex: 0 0 145px;
}

.result-wrap .result.first .articledetail .author {
  line-height: calc(100px / 2);
  text-align: center;
}

.result-wrap .result.first .articledetail .points {
  line-height: 1.5;
  text-align: center;
}

@media (max-width:870px) {
  .result-wrap .result.first {
    height: 70px;
  }

  .result-wrap .result.first .goldpanel {
    flex: 0 0 70px;
    line-height: 70px;
    font-size: 40px;
  }

  .result-wrap .result.first .title {
    flex: 1 1 70%;
  }

  .result-wrap .result.first .title .awardtitle {
    flex: 1 1 40%;
    line-height: 70px;
    font-size: 140%;
  }

  .result-wrap .result.first .title a {
    flex: 1 1 60%;
    line-height: 70px;
    font-size: 130%;
  }

  .result-wrap .result.first .articledetail {
    flex: 0 0 150px;
  }

  .result-wrap .result.first .articledetail .author {
    line-height: calc(70px / 2);
  }

  .result-wrap .result.first .articledetail .points {
    font-size: 10px;
  }
}

@media (max-width:610px) {

  .result-wrap .result.first {
    height: unset;
    flex-wrap: wrap;
    padding: 1em;
  }

  .result-wrap .result.first .goldpanel {
    display: none;
  }

  .result-wrap .result.first .title {
    flex: 1 0 100%;
    flex-wrap: wrap;
  }

  .result-wrap .result.first .title .awardtitle {
    flex: 1 0 100%;
    line-height: 2;
    font-size: 120%;
  }

  .result-wrap .result.first .title a {
    flex: 1 0 100%;
    line-height: 2;
    font-size: 150%;
  }

  .result-wrap .result.first .articledetail {
    flex: 0 0 100%;
  }

  .result-wrap .result.first .articledetail .author {
    line-height: 3;
  }

  .result-wrap .result.first .articledetail .points {
    line-height: 1.5;
  }
}

#html-body #odialog-container a {
  text-shadow: unset;
}


.result-wrap .result.second {
  flex: 1 1 30%;
  max-width: 350px!important;
  text-align: center;
  border: solid .1px dimgray;
  margin: .25em;
  padding: 1em;
  box-sizing: border-box;
}

.result-wrap .result.second .awardtitle {
  font-size: 105%;
  line-height: 2;
}

.result-wrap .result.second > a {
  font-size: 125%;
  line-height: 3;
}

.result-wrap .result.second .author {
  display: inline-block;
  margin-bottom: 1em;
}

[[/module]]

